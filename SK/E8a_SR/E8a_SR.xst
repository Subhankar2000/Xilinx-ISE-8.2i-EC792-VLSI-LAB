set -tmpdir "./xst/projnav.tmp"
set -xsthdpdir "./xst"
run
-ifn E8a_SR.prj
-ifmt mixed
-ofn E8a_SR
-ofmt NGC
-p acr2
-top E8a_SR
-opt_mode Speed
-opt_level 1
-iuc NO
-lso E8a_SR.lso
-keep_hierarchy YES
-rtlview Yes
-hierarchy_separator /
-bus_delimiter <>
-case maintain
-verilog2001 YES
-fsm_extract YES -fsm_encoding Auto
-safe_implementation No
-mux_extract YES
-resource_sharing YES
-iobuf YES
-pld_mp YES
-pld_xp YES
-pld_ce YES
-wysiwyg NO
-equivalent_register_removal YES
